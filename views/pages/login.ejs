  <div class="js-page-layout layout">
    <div class="container-fluid secureHeader">
      <div class="secureHeader__content">
        <a title="Factorial" class="factorial-logo" href="https://demo.factorial.dev"><img alt="factorial logo"
            src="https://assets.demo.factorial.dev/assets/public/factorial-logo-972a50101b4c6c78db94e048aa4d63ccd86e4643884750e516f59d6b712bbb27.svg"
            width="83" height="20">
        </a>
        <div class="secureHeader__sign_link">
          <span class="secureHeader__sign_link__sign_description">
            You do not have an account?
          </span>
          <a class="secureHeader__sign_link__link" href="https://demo.factorialhr.com/get-started">Get started now</a>
        </div>
      </div>
    </div>

    <div class="form__wrapper">
      <div class="container-fluid">
        <div class="row center-xs">
          <div class="col-xs-12 col-sm-6 col-md-4 form__container-wrapper">
            <div class="form__container">
              <div class="form__header">
                <div class="header-block">
                  <div class="header-block__content">
                    <h1 class="header-block__title">Sign in</h1>
                    <p class="header-block__description">Welcome back</p>
                    <div class="header-block__line"></div>
                  </div>
                </div>

              </div>
              <h3 class="sso-title">Continue with...</h3>
              <ul class="sso-list">
                <li class="sso-list__item">
                  <a href="<%= googlePath %>"
                    class="button button--lightGrey--outline button--full-width" title="Continue with Google">
                    <div class="sso_button_content">
                      <img alt="Continue with Google" class="sso_button_logo lazyload lazyload--shown lazied" width="20"
                        height="20"
                        data-src="https://assets.demo.factorial.dev/assets/public/sso_provider_logos/google-d9ee3b6b542a9c4e89c5537750f20f42a83396601d71f18de2c12de58063a2fd.svg"
                        data-srcset=""
                        src="https://assets.demo.factorial.dev/assets/public/sso_provider_logos/google-d9ee3b6b542a9c4e89c5537750f20f42a83396601d71f18de2c12de58063a2fd.svg"
                        data-lazied="IMG">
                    </div>
                  </a>
                </li>
                <li class="sso-list__item">
                  <a href="<%= microsoftPath %>"
                    class="button button--lightGrey--outline button--full-width" title="Continue with Microsoft">
                    <div class="sso_button_content">
                      <img alt="Continue with Microsoft" class="sso_button_logo lazyload lazyload--shown lazied"
                        width="20" height="20"
                        data-src="https://assets.demo.factorial.dev/assets/public/sso_provider_logos/microsoft-6fc5cef7a098754feedf87be3f3bcb43c8cda6ac8581088c12894e8a7878e196.svg"
                        data-srcset=""
                        src="https://assets.demo.factorial.dev/assets/public/sso_provider_logos/microsoft-6fc5cef7a098754feedf87be3f3bcb43c8cda6ac8581088c12894e8a7878e196.svg"
                        data-lazied="IMG">
                    </div>
                  </a>
                </li>
              </ul>
              <div class="sso_separator">
                <span class="sso_separator_text">
                  or
                </span>
              </div>


              <form class="form" id="new_user" action="<%= emailPath %>" accept-charset="UTF-8" method="post">
                <div class="form__row">
                  <div class="text-input">
                    <input autofocus="autofocus" class="text-input__input" required="required" type="email"
                      name="user[email]" id="user_email">
                    <label class="text-input__label" for="user_email">Email</label>
                  </div>
                </div>
                <div class="form__row">
                  <div class="text-input">
                    <input autocomplete="off" class="text-input__input" required="required" type="password"
                      name="user[password]" id="user_password">
                    <label class="text-input__label" for="user_password">Password</label>
                  </div>
                </div>
                <div class="form__row row">
                  <div class="col-xs">
                    <input name="user[remember_me]" type="hidden" value="0" autocomplete="off">
                    <input class="checkbox"
                      type="checkbox" value="1" name="user[remember_me]" id="user_remember_me">
                    <label class="xs-label" for="user_remember_me">Remember me</label>
                  </div>
                  <div class="col-xs right-aligned">
                    <a class="xs-label" href="https://api.demo.factorial.dev/en/users/password/new">Forgot your password?</a>
                  </div>
                </div>
                <input type="submit" name="commit" value="Sign in"
                  class="js-submit button button--brand button--full-width button-primary button-mobile-size">
                <a class="form__saml_link" href="<%= ssoPath %>">Sign in with SAML SSO</a>
              </form>
            </div>
            <div class="form__footer">
              <ul class="form__footer-list">
                <li class="form__footer-list__item">
                  <a class="ghostButton ghostButton--terciary button-mobile-size"
                    href="https://demo.factorialhr.com/get-started">Get started now</a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>
</body>

</html>